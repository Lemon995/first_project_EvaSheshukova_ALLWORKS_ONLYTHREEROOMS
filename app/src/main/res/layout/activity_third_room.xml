<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".second_room_activity"
    android:background="@drawable/thirdroom"
    android:id="@+id/layout"
    android:gravity="center"
    android:orientation="vertical"
    >

    <ImageButton
        android:id="@+id/iv_crossbar"
        android:layout_width="359dp"
        android:layout_height="101dp"
        android:layout_marginStart="16dp"
        android:background="@drawable/crowbar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.563" />

    <ImageButton
        android:id="@+id/iv_closet"
        android:layout_width="87dp"
        android:layout_height="362dp"
        android:background="@drawable/closet"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.955" />

    <Button
        android:id="@+id/bt_goBack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="go back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/iv_closet"
        app:layout_constraintHorizontal_bias="0.712"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_crossbar"
        app:layout_constraintVertical_bias="0.876"
        android:backgroundTint="#FF0000"

        />


</androidx.constraintlayout.widget.ConstraintLayout>
